" Colors
colorscheme hybrid

" Font
set guifont=Hack:h14

" Space between lines
set linespace=4

" vim-airline
let g:airline_theme='hybridline'

" vim multiple cursors
set selection=inclusive

" Hide scrollbars
set guioptions-=r
set guioptions-=L

" No beeping
set vb t_vb=

" Disable dialog popups
set guioptions+=c

" Config
:set cursorline
