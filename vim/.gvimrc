" Colors
colorscheme hybrid

" Font
set guifont=Hack:h12

" Space between lines
set linespace=2

" Disable bold fonts
set t_md=

" vim multiple cursors
set selection=inclusive

" Hide scrollbars
set guioptions-=r
set guioptions-=L

" No beeping
set vb t_vb=

" Disable dialog popups
set guioptions+=c

" Config
:set cursorline
