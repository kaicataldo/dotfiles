{
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "rust",
    "html",
    "css",
    "markdown",
    "json",
    "yaml"
  ],
  "coc.preferences.formatOnType": false,
  "eslint.run": "onSave",
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "tsserver.enableJavascript": false,
  "prettier.requireConfig": true
}
